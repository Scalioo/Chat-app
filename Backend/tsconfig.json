{"compilerOptions": {
    "target": "es2016",
    "jsx": "preserve",
    "module": "commonjs",
    "allowJs": true,
    "outDir": "./dist",
    "typeRoots": ["./node_modules/@types" , "./type"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },

  "exclude": [
    "client",
    "dist",
    "node_modules"
  ]
}
